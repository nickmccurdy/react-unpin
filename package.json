{
	"type": "module",
	"name": "react-unpin",
	"version": "1.0.4",
	"description": "Unpin your framework's React version to install it locally",
	"keywords": [
		"cli",
		"dependencies",
		"diagnostics",
		"info",
		"next",
		"node",
		"react",
		"rsc",
		"server-rendering",
		"utility",
		"version"
	],
	"homepage": "https://tsdocs.dev/docs/react-unpin",
	"license": "ISC",
	"author": "Nick McCurdy <nick@nickmccurdy.com> (https://nickmccurdy.com/)",
	"funding": "https://github.com/sponsors/nickmccurdy",
	"exports": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"react-unpin": "dist/cli.js",
		"unpin": "dist/cli.js"
	},
	"repository": "nickmccurdy/react-unpin",
	"scripts": {
